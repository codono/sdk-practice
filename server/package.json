{
  "name": "aris-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "local": "cross-env NODE_ENV=local dotenv -e .env.local -- ts-node-dev  --respawn --files src/server.ts",
    "dev": "cross-env NODE_ENV=development dotenv -e .env.dev -- ts-node-dev --respawn --files src/server.ts",
    "start:local": "cross-env NODE_ENV=local dotenv -e .env.local -- node dist/server.js",
    "start:dev": "cross-env NODE_ENV=development dotenv -e .env.dev -- node dist/server.js",
    "start:prod": "node dist/server.js",
    "tsc": "tsc",
    "generate": "prisma generate",
    "migrate:save": "dotenv -e .env.local -- prisma migrate dev --preview-feature",
    "migrate": "dotenv -e .env.local -- prisma migrate deploy --preview-feature",
    "migrate:dev": "dotenv -e .env.dev -- prisma migrate deploy --preview-feature",
    "migrate:prod": "dotenv -e .env -- prisma migrate deploy --preview-feature",
    "dummy": "dotenv -e .env.local -- ts-node src/utils/dummy/video.ts",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx"
  },
  "dependencies": {
    "async": "^3.2.0",
    "aws-sdk": "^2.850.0",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "googleapis": "^68.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "slack-node": "^0.1.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
