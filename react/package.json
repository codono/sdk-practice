{
  "name": "@minimal/material-kit-react",
  "author": "Minimal",
  "licence": "UNLICENSED",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "manifest": "fbt-manifest --src src --enum-manifest src/fbt_build/.enum_manifest.json --src-manifest src/fbt_build/.src_manifest.json",
    "collect-fbts": "fbt-collect --pretty --manifest < src/fbt_build/.src_manifest.json > src/fbt_build/.source_strings.json",
    "collect-fbts:test": "fbt-collect --plugins @babel/plugin-syntax-flow --pretty --manifest < src/fbt_build/.src_manifest.json > src/fbt_build/.test_source_strings.json",
    "translate-fbts": "fbt-translate --jenkins --source-strings src/fbt_build/.source_strings.json --translations src/fbt_build/translations/en.json src/fbt_build/translations/ko.json > src/fbt_build/translatedFbts.json",
    "translate-fbts:single-file": "fbt-translate --jenkins --stdin < src/fbt_build/translation_input.json > src/fbt_build/src/fbt_build/translation_output.json",
    "clean-fbts": "rm src/fbt_build/.enum_manifest.json src/fbt_build/.src_manifest.json src/fbt_build/.source_strings.json src/fbt_build/.test_source_strings.json src/fbt_build/translatedFbts.json 2",
    "fbt:all": "yarn manifest && yarn collect-fbts && yarn translate-fbts",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11",
      "not chrome < 51",
      "not safari < 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@emotion/cache": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@iconify/icons-ant-design": "^1.1.0",
    "@iconify/icons-eva": "^1.1.0",
    "@iconify/icons-ic": "^1.1.3",
    "@iconify/icons-vscode-icons": "^1.1.1",
    "@iconify/react": "^1.1.4",
    "@material-ui/core": "^5.0.0-alpha.30",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^5.0.0-alpha.30",
    "@material-ui/system": "^5.0.0-alpha.29",
    "@material-ui/utils": "^5.0.0-alpha.29",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "change-case": "^4.1.2",
    "date-fns": "^2.21.1",
    "deck.gl": "^8.4.13",
    "faker": "^5.5.3",
    "fbt": "^0.16.5",
    "framer-motion": "^4.1.4",
    "history": "^4.10.1",
    "i18next": "^20.0.0",
    "i18next-browser-languagedetector": "^6.1.0",
    "jss": "^10.5.1",
    "jss-rtl": "^0.3.0",
    "lodash": "^4.17.19",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-i18next": "^11.8.13",
    "react-intersection-observer": "^8.31.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-scroll": "^1.8.1",
    "recoil": "^0.2.0",
    "simplebar": "^5.3.0",
    "simplebar-react": "^2.3.0",
    "styled-components": "^5.2.3",
    "stylis": "^4.0.9",
    "stylis-plugin-rtl": "^2.0.2",
    "typescript": "^4.2.3",
    "web-vitals": "^1.q0.1",
    "babel-loader": "8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@fbtjs/default-collection-transform": "^0.0.3",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.18",
    "@types/faker": "^5.5.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.0.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-scroll": "^1.8.2",
    "@types/recoil": "^0.0.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-plugin-fbt": "^0.20.0",
    "babel-plugin-fbt-runtime": "^0.9.17",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.7.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "fbt-generate-translations": "^0.0.4",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.1",
    "babel-loader": "8.1.0"
  }
}
